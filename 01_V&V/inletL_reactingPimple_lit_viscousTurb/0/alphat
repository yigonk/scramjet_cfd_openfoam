FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      alphat;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    P.Inlet
    {
        type        calculated;
        value       uniform 0;
    }

    P.Outlet
    {
        type        calculated;
        value       uniform 0;
    }

    Farfield
    {
        type        zeroGradient;
    }

    Wall
    {
        type        compressible::alphatWallFunction;
        Prt         0.9;
        value       uniform 0;
    }

    Symmetry
    {
        type        symmetryPlane;
    }

    patch0
    {
        type        empty;
    }

    patch3
    {
        type        empty;
    }
}

