FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

// nut â‰ˆ k/omega using the inlet estimates above
internalField   uniform 0.00531525;

boundaryField
{
    P.Inlet
    {
        type        calculated;
        value       uniform 0.00531525;
    }

    P.Outlet
    {
        type        zeroGradient;
    }

    Farfield
    {
        type        zeroGradient;
    }

    Wall
    {
        type            nutkWallFunction;
        value           uniform 0;
    }

    Symmetry
    {
        type        symmetryPlane;
    }

    patch0
    {
        type        empty;
    }

    patch3
    {
        type        empty;
    }
}

