FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    "(rho|U|h|p|Ydefault).*"
    {
        solver          PBiCGStab;
        preconditioner  DILU;
        tolerance       1e-08;
        relTol          0.0;
    }

    omega
    {
        solver          PBiCGStab;
        preconditioner  DILU;
        tolerance       1e-8;
        relTol          0.1;
    }

    k
    {
        solver          PBiCGStab;
        preconditioner  DILU;
        tolerance       1e-8;
        relTol          0.1;
    }

    omegaFinal
    {
        solver          PBiCGStab;
        preconditioner  DILU;
        tolerance       1e-8;
        relTol          0;
    }

    kFinal
    {
        solver          PBiCGStab;
        preconditioner  DILU;
        tolerance       1e-8;
        relTol          0;
    }


}

PIMPLE
{
    momentumPredictor false;
    nCorrectors 1;
    nOuterCorrectors 3;
    nonOrthogonalityThreshold 60;
    skewnessThreshold 0.5;
    updateEnergyInPISO false;
    fluxScheme Kurganov;

    kappaFunction
    {
        type   standardMachToAcCourantRatio;

        standardMachToAcCourantRatioCoeffs {};
    }

}


